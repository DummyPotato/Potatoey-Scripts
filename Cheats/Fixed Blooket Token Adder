async function getName(t){const o=await fetch("https://api.blooket.com/api/users/verify-token?token=JWT+"+t);return(await o.json()).name}async function addCurrencies(){const t=Number(prompt("How many tokens do you want to add to your account? (500 daily)")),o=localStorage.token.split("JWT ")[1];t>500&&alert("You can only add up to 500 tokens daily."),200==(await fetch("https://api.blooket.com/api/users/add-rewards",{method:"PUT",headers:{referer:"https://www.blooket.com/","content-type":"application/json",authorization:localStorage.token},body:JSON.stringify({addedTokens:t,addedXp:300,name:await getName(o)})})).status?alert(`${t} tokens and 300 XP added to your account!`):alert("An error occured.")}addCurrencies();
